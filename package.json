{
  "name": "mskew",
  "version": "1.0.0",
  "description": "Spotify Player using React + Webpack + Express",
  "main": "src/index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "start:dev": "better-npm-run start:dev",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "babel-node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command": "babel-node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GomezMimo/spotify-app"
  },
  "keywords": [
    "react",
    "webpack",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GomezMimo/spotify-app/issues"
  },
  "homepage": "https://github.com/GomezMimo/spotify-app#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-audio-player": "^0.3.3",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.0-rc5",
    "spotify-web-api-js": "^0.21.2",
    "spotify-web-api-node": "^2.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.22.0",
    "better-npm-run": "0.0.15",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^1.0.10",
    "browser-sync": "^2.18.8",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "react-router-dom": "^4.0.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-md5-hash": "0.0.5"
  }
}
